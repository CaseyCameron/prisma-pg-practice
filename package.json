{
  "name": "express-mongodb-starter",
  "version": "1.0.0",
  "description": "express mongodb app template",
  "main": "src/server.ts",
  "scripts": {
    "dev": "nodemon ./src/server.ts",
    "build": "babel src --out-dir build --extensions .ts --source-maps inline",
    "start": "node ./build/server.js",
    "test": "jest --verbose",
    "lint": "eslint ./src",
    "format:check": "npx prettier --check .",
    "format:write": "npx prettier --write ."
  },
  "author": "Mike Pottebaum",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "mongodb": "^4.12.1",
    "mongoose": "^6.8.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/node": "^7.20.5",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/morgan": "^1.9.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "jest": "^29.3.1",
    "mongodb-memory-server": "^8.10.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
