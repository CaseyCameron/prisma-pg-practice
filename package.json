{
  "name": "backend-pg-prisma",
  "version": "1.0.0",
  "description": "express app template",
  "main": "src/server.ts",
  "scripts": {
    "dev": "nodemon ./src/server.ts",
    "build": "babel src --out-dir build --extensions .ts --source-maps inline",
    "start": "node ./build/server.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "migrate:postgres": "dotenv -e .env.test -- npx prisma migrate deploy",
    "prisma:reset": "npx prisma migrate reset -f",
    "test": "npm run docker:up && npm run prisma:reset && npm run migrate:postgres && jest -i --verbose --watch",
    "lint": "eslint ./src",
    "format:check": "npx prettier --check .",
    "format:write": "npx prettier --write ."
  },
  "author": "Mike Pottebaum",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.1.0",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "morgan": "^1.10.0",
    "pg": "^8.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/node": "^7.20.5",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/morgan": "^1.9.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "express-validator": "^6.15.0",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "prisma": "^4.11.0",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
